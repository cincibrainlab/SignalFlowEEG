{
      "name": "Matlab R2021b",
      "dockerComposeFile": "docker-compose.yml",
      "service": "my-service",
      "workspaceFolder": "/workspace",
      "customizations": {
            "vscode": {
                  "settings": {
                        "python.pythonPath": "/usr/local/bin/python"
                  },
                  "extensions": [
                        "ms-python.python",
                        "dbaeumer.vscode-eslint"
                  ]
            }
      },
      "forwardPorts": [6080],
      "portsAttributes": {
            "6080": {"label": "noVNC Ubuntu Desktop Root Display", "onAutoForward": "openBrowser"}
      }
}